{% extends 'base.html.twig' %}

{% block body %}


    <tbody>


    <div class="row">
        <div class="col-md-offset-2  col-md-8 fondblanc">

            <div class="row">
                <div class="col-md-12">
                    <h1 class="lienstitre">Astreintes</h1>
                </div>
            </div>

            <div class="row">
                <div class="col-md-2">


                    <!--Début archives astreintes-->


                    <div class="dropdown">
                        <button class="btn btn-default dropdown-toggle astreintesbtn" type="button" id="dropdownMenu1" data-toggle="dropdown">
                            Administratif
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <div class="container-fluid">
                                {% set maxId = 0 %}
                                {% for astreintes in astreintes %}
                                    {% set maxId = max(astreintes.id, maxId) %}
                                {% endfor %}
                                    {# Affichage des informations ne correspondant pas à l'Id sauvegarder#}
                                    {% for astreintes in astreintes %}
                                        {% if astreintes.id != maxId %}
                                            {% if astreintes.titleadministratif != NULL %}
                                                <p data-toggle="modal" data-target=".bs-example-modal-lg1" class="astreintesarchivestitres"> {{ astreintes.titleadministratif }} </p>
                                            {% endif %}
                                        {% endif %}
                                    {% endfor %}
                            </div>
                        </ul>
                    </div>

                    <div class="dropdown">
                        <button class="btn btn-default dropdown-toggle astreintesbtn" type="button" id="dropdownMenu1" data-toggle="dropdown">
                            Médical
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <div class="container-fluid">
                                {% set maxId = 0 %}
                                {% for astreintes in astreintes %}
                                    {% set maxId = max(astreintes.id, maxId) %}
                                {% endfor %}
                                {# Affichage des informations ne correspondant pas à l'Id sauvegarder#}
                                {% for astreintes in astreintes %}
                                    {% if astreintes.id != maxId %}
                                        {% if astreintes.titlemedical != NULL %}
                                            <p data-toggle="modal" data-target=".bs-example-modal-lg2" class="astreintesarchivestitres"> {{ astreintes.titlemedical }} </p>
                                        {% endif %}
                                    {% endif %}
                                {% endfor %}
                            </div>
                        </ul>
                    </div>

                    <div class="dropdown">
                        <button class="btn btn-default dropdown-toggle astreintesbtn" type="button" id="dropdownMenu1" data-toggle="dropdown">
                            Commune
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <div class="container-fluid">
                                {% set maxId = 0 %}
                                {% for astreintes in astreintes %}
                                    {% set maxId = max(astreintes.id, maxId) %}
                                {% endfor %}
                                {# Affichage des informations ne correspondant pas à l'Id sauvegarder#}
                                {% for astreintes in astreintes %}
                                    {% if astreintes.id != maxId %}
                                        {% if astreintes.titlecommune != NULL %}
                                            <p data-toggle="modal" data-target=".bs-example-modal-lg3" class="astreintesarchivestitres"> {{ astreintes.titlecommune }} </p>
                                        {% endif %}
                                    {% endif %}
                                {% endfor %}
                            </div>
                        </ul>
                    </div>

                    <div class="dropdown">
                        <button class="btn btn-default dropdown-toggle astreintesbtn" type="button" id="dropdownMenu1" data-toggle="dropdown">
                            Technique
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                            <div class="container-fluid">
                                {% set maxId = 0 %}
                                {% for astreintes in astreintes %}
                                    {% set maxId = max(astreintes.id, maxId) %}
                                {% endfor %}
                                {# Affichage des informations ne correspondant pas à l'Id sauvegarder#}
