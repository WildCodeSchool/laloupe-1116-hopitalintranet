                            </object>
                        {% endif %}
                    {% endfor %}
                </div>
            </div>

            <!--Fin nouvelles astreintes-->




            <!--Début modal-->



            <div class="modal fade bs-example-modal-lg1" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-body" style="height: 700px">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <br>
                            <br>
                            <br>
                            {% for astreintes in astreintes %}
                                {% if astreintes.id != maxId %}
                                            <object data="{{ app.request.basepath }}/uploads/{{ astreintes.administratifimg }}" type="application/pdf" class="menuremplacepdf">
                                            </object>
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>


            <div class="modal fade bs-example-modal-lg2" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-body" style="height: 700px">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <br>
                            <br>
                            <br>
                            {% for astreintes in astreintes %}
                                {% if astreintes.id != maxId %}
                                    <object data="{{ app.request.basepath }}/uploads/{{ astreintes.medicalimg }}" type="application/pdf" class="menuremplacepdf">
                                    </object>
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>


            <div class="modal fade bs-example-modal-lg3" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-body" style="height: 700px">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <br>
                            <br>
                            <br>
                            {% for astreintes in astreintes %}
                                {% if astreintes.id != maxId %}
                                    <object data="{{ app.request.basepath }}/uploads/{{ astreintes.communeimg }}" type="application/pdf" class="menuremplacepdf">
                                    </object>
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>


            <div class="modal fade bs-example-modal-lg4" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content">
                        <div class="modal-body" style="height: 700px">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <br>
                            <br>
                            <br>
                            {% for astreintes in astreintes %}
                                {% if astreintes.id != maxId %}
                                    <object data="{{ app.request.basepath }}/uploads/{{ astreintes.techniqueimg }}" type="application/pdf" class="menuremplacepdf">
                                    </object>
                                {% endif %}
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>






            <!--Fin modal-->












        </div>
    </div>















    </tbody>

{% endblock %}


