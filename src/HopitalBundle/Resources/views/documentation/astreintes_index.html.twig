                                    {% if astreintes.id != maxId %}
                                        {% if astreintes.titletechnique != NULL %}
                                            <p data-toggle="modal" data-target=".bs-example-modal-lg4" class="astreintesarchivestitres"> {{ astreintes.titletechnique }} </p>
                                        {% endif %}
                                    {% endif %}
                                {% endfor %}
                            </div>
                        </ul>
                    </div>

                </div>


                <!--Fin archives astreintes-->


                <!--Début nouvelles astreintes-->


                <div class="col-md-5">
                    {% set maxId = 0 %}
                    {% for astreintes in astreintes %}
                        {% set maxId = max(astreintes.id, maxId) %}
                    {% endfor %}
                    {% for astreintes in astreintes %}
                        {% if astreintes.id == maxId %}
                            <p>{{ astreintes.titleadministratif }}</p>
                        {% endif %}
                        {% if astreintes.id == maxId %}
                            <object data="{{ app.request.basepath }}/uploads/{{ astreintes.administratifimg }}" type="application/pdf" class="astreintesimg">
                            </object>
                        {% endif %}
                    {% endfor %}
                </div>


                <div class="col-md-5">
                    {% set maxId = 0 %}
                    {% for astreintes in astreintes %}
                        {% set maxId = max(astreintes.id, maxId) %}
                    {% endfor %}
                    {% for astreintes in astreintes %}
                        {% if astreintes.id == maxId %}
                            <p>{{ astreintes.titlemedical }}</p>
                        {% endif %}
                        {% if astreintes.id == maxId %}
