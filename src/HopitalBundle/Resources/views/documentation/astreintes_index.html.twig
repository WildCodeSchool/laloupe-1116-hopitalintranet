                            </object>
                        {% endif %}
                    {% endfor %}
                </div>
            </div>

            <div class="row">
                <div class="col-md-offset-2 col-md-5">
                    {% set maxId = 0 %}
                    {% for astreintes in astreintes %}
                        {% set maxId = max(astreintes.id, maxId) %}
                    {% endfor %}
                    {% for astreintes in astreintes %}
                        {% if astreintes.id == maxId %}
                            <p>{{ astreintes.titlecommune }}</p>
                        {% endif %}
                        {% if astreintes.id == maxId %}
                            <object data="{{ app.request.basepath }}/uploads/{{ astreintes.communeimg }}" type="application/pdf" class="astreintesimg">
                            </object>
                        {% endif %}
                    {% endfor %}
                </div>


                <div class="col-md-5">
                    {% set maxId = 0 %}
                    {% for astreintes in astreintes %}
                        {% set maxId = max(astreintes.id, maxId) %}
                    {% endfor %}
                    {% for astreintes in astreintes %}
                        {% if astreintes.id == maxId %}
