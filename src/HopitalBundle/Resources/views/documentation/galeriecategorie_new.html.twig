{% extends 'base.html.twig' %}

{% block body %}
<div class="row">
    <div class="col-md-offset-2 col-md-8 fondblanc">
        <h1 class="lienstitrenew">Création d'un dossier photo</h1>
        <div class="row">
            <div class="col-md-offset-3 col-md-6 carrenew">
                <div class="row">
                    <div class="col-md-12 imgnewid">
                        {{ form_start(form) }}
                        {{ form_label(form.categorie, 'Nommez votre dossier:') }}<br>
                        {{ form_widget(form.categorie)}}<br><br>
                        <div class="row">
                            <div class="col-md-offset-5 col-md-2">
                                <input type="submit" value="Créer" class="btnnew" />
                            </div>
                        </div>
                        {{ form_end(form) }}
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
{% endblock %}
