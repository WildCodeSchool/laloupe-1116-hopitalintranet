{% extends 'base.html.twig' %}

{% block body %}
    <h1>Journaux</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ journaux.id }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('documentation_journaux_index') }}">Back to the list</a>
        </li>
